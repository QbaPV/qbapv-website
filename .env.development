# .env.development
# Variables de entorno para desarrollo local

# API Configuration
REACT_APP_API_URL=http://localhost:5001
REACT_APP_API_TIMEOUT=10000

# reCAPTCHA Configuration
# Para desarrollo local, usar la clave de prueba de Google o dejar vacío
# REACT_APP_RECAPTCHA_SITE_KEY=6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI
REACT_APP_RECAPTCHA_SITE_KEY=

# Clave de producción (no usar en localhost)
# REACT_APP_RECAPTCHA_SITE_KEY_PROD=6LeDKlIqAAAAAH_qOOh3XQPwemwAj9V5MhPqvH60

# Environment
NODE_ENV=development
REACT_APP_ENV=development

# Debug
REACT_APP_DEBUG=true
REACT_APP_SHOW_LOGS=true

# Features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_HOTJAR=false

# Social Media
REACT_APP_FACEBOOK_URL=https://facebook.com/qbapv
REACT_APP_TWITTER_URL=https://twitter.com/qbapv
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/qbapv
REACT_APP_INSTAGRAM_URL=https://instagram.com/qbapv
REACT_APP_YOUTUBE_URL=https://youtube.com/@qbapv

# Contact Information
REACT_APP_CONTACT_EMAIL=info@qbapv.com
REACT_APP_CONTACT_PHONE=+1-555-0123
REACT_APP_CONTACT_ADDRESS=123 Business St, City, Country